{
  "name": "cheeseplate",
  "version": "1.0.0",
  "description": "Bot with functionality that Telegram lacks",
  "scripts": {
    "test": "vitest --config config/vite.config.ts",
    "lint": "eslint .",
    "format-check": "prettier -c .",
    "type-check": "tsc --noEmit",
    "standalone": "node scripts/run-standalone.js",
    "dev:nowatch": "node -r ts-node/register -r tsconfig-paths/register src/bot/index.ts",
    "dev": "nodemon nodemon.json",
    "prisma": "prisma",
    "tsc": "tsc",
    "build:serverless": "SERVERLESS_BUILD=true tsup",
    "build:standalone": "STANDALONE_BUILD=true tsup",
    "vercel-build": "npm run build:serverless && npm run prisma migrate deploy",
    "botfather-commands": "zx ./scripts/botfather-commands.mjs",
    "set-webhook": "node ./scripts/set-telegram-webhook.js",
    "dep-graph": "depcruise --ts-config tsconfig.json --include-only \"^src\" --output-type dot src | dot -T png > dependencygraph.png"
  },
  "author": "Vitaliy Lyovkin <web@roamiiing.ru> (https://roamiiing.ru)",
  "license": "GPL-3.0",
  "devDependencies": {
    "@sentry/node": "^7.28.1",
    "@sentry/tracing": "^7.28.1",
    "@types/json-bigint": "^1.0.1",
    "@types/luxon": "^3.0.0",
    "@types/node": "^18.7.14",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.3",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.10.2",
    "@vercel/node": "^2.8.3",
    "@vitest/ui": "^0.26.3",
    "awilix": "^8.0.0",
    "axios": "^1.2.2",
    "c8": "^7.11.0",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^3.4.1",
    "eslint-plugin-import": "^2.25.4",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "prettier-plugin-prisma": "^4.8.0",
    "prisma": "^4.4.0",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^4.1.2",
    "tsup": "^6.2.3",
    "typescript": "^4.5.4",
    "vitest": "^0.26.3",
    "yargs": "^17.6.0",
    "zx": "^7.1.1"
  },
  "dependencies": {
    "@prisma/client": "^4.4.0",
    "consola": "^2.15.3",
    "defu": "^6.1.0",
    "dotenv": "^16.0.1",
    "fp-ts": "^2.11.8",
    "grammy": "^1.13.0",
    "json-bigint": "^1.0.0",
    "luxon": "^3.0.1",
    "metriqa": "^0.1.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "source-map-support": "^0.5.21",
    "telegraf": "^4.8.1",
    "zod": "^3.14.2"
  }
}
